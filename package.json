{
    "name": "get-wild",
    "version": "1.4.0",
    "description": "Pluck nested properties from an object with support for wildcards",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.umd.min.js",
    "jsdelivr": "dist/index.umd.min.js",
    "types": "dist/index.d.ts",
    "repository": "https://github.com/chocolateboy/get-wild",
    "author": "chocolateboy",
    "license": "Artistic-2.0",
    "scripts": {
        "build": "rollup -c --exports auto",
        "build:doc": "toc-md README.md",
        "build:release": "cross-env NODE_ENV=production run-s typecheck build build:doc",
        "clean": "shx rm -rf dist .nyc_output",
        "coverage": "nyc ava --verbose \"./test/**/*.js\"",
        "prepublishOnly": "run-s clean build:release test:run",
        "rebuild": "run-s clean build",
        "repl": "node -r ./resources/repl.js",
        "test:run": "ava --verbose \"./test/**/*.js\"",
        "test": "run-s build coverage",
        "typecheck": "tsc --noEmit --noImplicitAny --noUnusedLocals --noUnusedParameters --strict"
    },
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.esm.js"
        },
        "./": "./dist/",
        "./package.json": "./package.json"
    },
    "files": [
        "dist/*.{js,d.ts}"
    ],
    "browserslist": "maintained node versions",
    "devDependencies": {
        "@babel/core": "^7.13.8",
        "@types/node": "^14.14.31",
        "@wessberg/rollup-plugin-ts": "^1.3.8",
        "array-flat-polyfill": "^1.0.1",
        "ava": "^3.15.0",
        "babel-plugin-codegen": "^4.1.4",
        "babel-plugin-source-map-support": "^2.1.3",
        "cross-env": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "rollup": "^2.40.0",
        "rollup-plugin-filesize": "^9.1.1",
        "rollup-plugin-terser": "^7.0.2",
        "shx": "^0.3.3",
        "source-map-support": "^0.5.19",
        "toc-md-alt": "^0.4.1",
        "typescript": "^4.2.2"
    },
    "keywords": [
        "dig",
        "dot",
        "dot-notation",
        "dotted",
        "dotty",
        "extract",
        "get",
        "glob",
        "json",
        "json-selector",
        "lodash-get",
        "path",
        "paths",
        "pluck",
        "query",
        "selector",
        "star",
        "step",
        "steps",
        "wildcard",
        "wildcards"
    ]
}
