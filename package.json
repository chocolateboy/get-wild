{
    "name": "get-wild",
    "version": "0.0.1",
    "description": "Pluck nested properties from an object with support for wildcards",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.umd.min.js",
    "jsdelivr": "dist/index.umd.min.js",
    "types": "dist/index.d.ts",
    "repository": "https://github.com/chocolateboy/get-wild",
    "author": "chocolateboy",
    "license": "Artistic-2.0",
    "scripts": {
        "build": "bili --map --format cjs -d dist src/index.ts",
        "build:default": "cross-env NODE_ENV=production bili --no-map --format cjs,esm -d dist src/index.ts",
        "build:dts": "npm-dts generate -o dist/index.d.ts",
        "build:min": "cross-env NODE_ENV=production bili --no-map --format esm-min -d dist src/index.ts",
        "build:release": "run-s typecheck build:default build:umd build:dts",
        "build:umd": "cross-env NODE_ENV=production bili --no-map --module-name GetWild --format umd-min -d dist src/index.ts",
        "clean": "shx rm -rf dist",
        "coverage": "run-s build nyc",
        "doctoc": "markdown-toc --bullets=- --maxdepth 2 -i README.md",
        "nyc": "nyc ava --verbose ./test/*.js",
        "prepublishOnly": "run-s clean build:release nyc",
        "rebuild": "run-s clean build",
        "repl": "node -r ./resources/repl.js",
        "test": "run-s typecheck build test:run",
        "test:run": "ava --verbose ./test/*.js",
        "typecheck": "tsc --noEmit --noImplicitAny --noUnusedLocals --noUnusedParameters --strict"
    },
    "files": [
        "dist/index.d.ts",
        "dist/index.esm.js",
        "dist/index.js",
        "dist/index.umd.min.js"
    ],
    "browserslist": "maintained node versions",
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "array-flat-polyfill": "^1.0.1",
        "ava": "^3.12.1",
        "babel-plugin-codegen": "^4.0.1",
        "babel-plugin-source-map-support": "^2.1.3",
        "bili": "^5.0.5",
        "cross-env": "^7.0.2",
        "markdown-toc": "^1.2.0",
        "npm-dts": "^1.3.4",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "rollup-plugin-typescript2": "^0.27.2",
        "shx": "^0.3.2",
        "typescript": "^4.0.2"
    },
    "keywords": [
        "dig",
        "dotted",
        "dot",
        "extract",
        "get",
        "glob",
        "json",
        "json-selector",
        "lodash-get",
        "paths",
        "path",
        "pluck",
        "query",
        "selector",
        "star",
        "steps",
        "step",
        "wildcard",
        "wildcards"
    ]
}
